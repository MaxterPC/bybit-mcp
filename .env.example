# Bybit API credentials
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here

# Set to "false" for mainnet (production)
BYBIT_TESTNET=true

# Bearer token to protect the MCP endpoint
MCP_AUTH_TOKEN=your_secure_random_token_here

# Server port (Cloud Run sets this automatically)
PORT=8080
